        },
        "bind": "name"
      },
      "任务编号": {
        "bind": "plan_sn",
        "view": {
          "type": "Text",
          "props": {}
        },
        "edit": {
          "props": {},
          "type": "Input",
          "bind": "plan_sn"
        }
      },
      "类型": {
        "edit": {
          "bind": "type",
          "props": {
            "options": [
              {
                "labe": "::项目",
                "value": "项目"
              }
            ],
            "placeholder": "请选择"
          },
          "type": "Select"
        },
        "bind": "type"
      },
      "工程数": {
        "bind": "total_number",
