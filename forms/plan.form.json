            },
            {
              "name": "任务编号",
              "width": 12
            },
            {
              "name": "类型",
              "width": 12
            },
            {
              "name": "工程数",
              "width": 12
            },
            {
              "name": "序号",
              "width": 12
            },
            {
              "name": "状态",
              "width": 12
            },
            {
              "name": "进度%",
              "width": 12
            },
            {
              "name": "负责人",
              "width": 12
            },
            {
              "name": "负责部门",
              "width": 12
