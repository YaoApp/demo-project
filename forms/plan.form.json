          "props": {
            "type": "number"
          },
          "type": "Input",
          "bind": "order"
        }
      },
      "状态": {
        "edit": {
          "props": {
            "options": [
              {
                "labe": "::进行中",
                "value": "进行中"
              },
              {
                "labe": "::已完成",
                "value": "已完成"
              }
            ],
            "placeholder": "请选择"
          },
          "type": "Select"
        },
        "bind": "status"
      },
      "负责人2": {
        "edit": {
          "props": {
            "xProps": {
              "$remote": {
                "process": "scripts.contact.user.GetSelect",
                "query": {}
              }
            }
          },
          "type": "Select",
          "bind": "user_id"
        },
        "bind": "user_id"
      },
      "负责人": {
        "edit": {
          "props": {
            "mode": "multiple",
            "xProps": {
              "$remote": {
                "process": "scripts.contact.user.TagSelect",
                "query": {}
              }
            }
          },
          "type": "Select",
          "bind": "users"
        },
        "bind": "users"
      },
      "负责部门": {
        "edit": {
          "props": {
            "mode": "multiple",
            "xProps": {
              "$remote": {
                "process": "scripts.contact.department.TagSelect",
                "query": {}
              }
            }
          },
          "type": "Select",
          "bind": "department"
        },
        "bind": "department"
      },
      "进度%": {
        "edit": {
          "bind": "progress",
          "props": {
            "type": "number"
          },
          "type": "Input"
        },
        "bind": "progress",
        "view": {
          "props": {},
          "type": "Text"
        }
      },

      "预算金额": {
        "edit": {
          "bind": "total_money",
          "props": {
            "type": "number"
          },
          "type": "Input"
        },
        "bind": "total_money",
        "view": {
          "props": {},
          "type": "Text"
        }
      },
      "计划开始时间": {
        "edit": {
          "type": "DatePicker",
          "bind": "start_time",
          "props": {}
        },
        "bind": "start_time",
        "view": {
          "props": {
            "format": "YYYY-MM-DD"
          },
          "type": "Text"
        }
      },
      "计划结束时间": {
        "edit": {
          "type": "DatePicker",
          "bind": "end_time",
          "props": {}
        },
        "bind": "end_time",
        "view": {
          "props": { "format": "YYYY-MM-DD" },
          "type": "Text"
        }
      },
      "实际结束时间": {
        "edit": {
          "type": "DatePicker",
          "bind": "actual_time",
          "props": {}
        },
        "bind": "actual_time",
        "view": {
          "props": { "format": "YYYY-MM-DD" },
          "type": "Text"
        }
      }
